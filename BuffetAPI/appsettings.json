{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "NoEquipe": "Équipe du tonnerre" // Remplacer par votre numéro d'équipe
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BuffetAPIContext": "Server=(localdb)\\mssqllocaldb;Database=BuffetAPIContext03;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}