{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "NoEquipe": "Équipe du tonnerre" // Remplacer par votre numéro d'équipe
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BuffetAPIContext": "Server=(localdb)\\mssqllocaldb;Database=BuffetAPIContext-2482c4ba-f1ff-48f1-8df1-5b8cd442c7f7;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "JWT": {
    "Issuer": "BuffetAPI",
    "Audience": "BuffetAPIClient",
    "DurationInMinutes": 45,
    "Secret": "Ma super clé secrète pour signer des jetons JWT"
  }

}